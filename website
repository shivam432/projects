    <script>
        var p, q, r, s;
        p = document.getElementsByid("num1");
        q = document.getElementsByid("num2");
        r = document.getElementsByid("num3");
        s = document.getElementsByid("num");
        while ((p && q && r && s) < 10000) {
            setInterval(inc, 100);
            setInterval(inc, 100);
            setInterval(inc, 100);
            setInterval(inc, 100);
        }
        function inc() {
            p = p + 1;
            q = q + 1;
            r = r + 1;
            s = s + 1;
            document.getElementsByid("num1").innerHTML = p;
            document.getElementsByid("num2").innerHTML = q;
            document.getElementsByid("num3").innerHTML = r;
            document.getElementsByid("num").innerHTML = s;
        }
    </script>
